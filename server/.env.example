# DataLens AI Server Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development

# PostgreSQL Database Configuration (for storing app data)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=datalens_ai
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_SSL=false
DB_POOL_MAX=20
DB_IDLE_TIMEOUT=30000

# Auto-run migrations on server start
AUTO_RUN_MIGRATIONS=true
